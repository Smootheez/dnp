{
  "schemaVersion": 1,
  "id": "${modid}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "Displays floating number particles whenever an entity takes damage or receives healing",
  "authors": [
    "Smootheez"
  ],
  "contact": {
    "homepage": "https://github.com/Smootheez/dnp",
    "sources": "https://github.com/Smootheez/dnp",
    "issues": "https://github.com/Smootheez/dnp/issues"
  },
  "license": "GNU General Public License v3.0",
  "icon": "assets/dnp/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "dev.smootheez.dnp.Dnp"
    ],
    "scl" : [
      "dev.smootheez.dnp.config.DnpConfig"
    ]
  },
  "mixins": [
    "dnp.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": ">=${minecraft_version}",
    "scl": ">=${scl_version}"
  }
}